    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/books?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value="root"/>
    </properties>
  </persistence-unit>
  <persistence-unit name="view_issue_book?zeroDateTimeBehavior=convertToNullPU" transaction-type="RESOURCE_LOCAL">
